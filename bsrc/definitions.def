DEFINITIONS
    AttributeID_Node_Variant == ({
        /* mandatory attributes: */
        e_aid_AccessLevel |-> (a_AccessLevel;conv_AccessLevel_Variant),
        e_aid_BrowseName |-> (a_BrowseName;conv_QualifiedName_Variant),
        e_aid_ContainsNoLoops |-> (a_ContainsNoLoops;conv_Boolean_Variant),
        e_aid_DataType |-> (a_DataType;conv_NodeId_Variant),
        e_aid_DisplayName |-> (a_DisplayName;conv_LocalizedText_Variant),
        e_aid_EventNotifier |-> (a_EventNotifier;conv_Byte_Variant),
        e_aid_Executable |-> (a_Executable;conv_Boolean_Variant),
        e_aid_Historizing |-> (a_Historizing;conv_Boolean_Variant),
        e_aid_IsAbstract |-> (a_IsAbstract;conv_Boolean_Variant),
        e_aid_NodeClass |-> (a_NodeClass;conv_NodeClass_Variant),
        e_aid_NodeId |-> (a_NodeId;conv_NodeId_Variant),
        e_aid_Symmetric |-> (a_Symmetric;conv_Boolean_Variant),
        e_aid_UserAccessLevel |-> (a_UserAccessLevel;conv_AccessLevel_Variant),
        e_aid_UserExecutable |-> (a_UserExecutable;conv_Boolean_Variant),
        e_aid_Value |-> a_Value,
        e_aid_ValueRank |-> (a_ValueRank;conv_ValueRank_Variant),
        e_aid_ArrayDimensions |-> (a_ArrayDimensions;conv_ArrayDimensions_Variant)
    })
